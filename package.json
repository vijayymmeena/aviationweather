{
  "name": "aviationweather",
  "version": "4.0.1",
  "description": "Node.js wrapper for aviation weather, written in TypeScript",
  "keywords": [
    "aviation",
    "flight",
    "node",
    "wrapper",
    "weather"
  ],
  "homepage": "https://github.com/vijayymmeena/aviationweather#readme",
  "bugs": {
    "url": "https://github.com/vijayymmeena/aviationweather/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vijayymmeena/aviationweather.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Vijay Meena <vijayymmeena@gmail.com> (https://github.com/vijayymmeena)"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "SECURITY.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "format": "biome format --write",
    "lint": "biome lint",
    "test": "tsx tests/test.ts"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.0",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.9.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.19.0"
}
